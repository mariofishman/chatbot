{
  "designSystemProfile": {
    "style": {
      "typography": {
        "fontFamily": "system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif",
        "baseFontSize": "16px",
        "lineHeight": {
          "default": 1.5,
          "tight": 1.2,
          "relaxed": 1.6
        },
        "weights": {
          "regular": 400,
          "medium": 500,
          "semibold": 600,
          "bold": 700
        }
      },
      "colors": {
        "primary": {
          "purple": "#8A63D2",
          "orange": "#FF9500",
          "green": "#34C759",
          "blue": "#007AFF",
          "yellow": "#FFCC00",
          "gray": "#8E8E93",
          "red": "#FF3B30",
          "pink": "#FF69B4"
        },
        "neutral": {
          "white": "#FFFFFF",
          "lightGray": "#F2F2F7",
          "mediumGray": "#8E8E93",
          "darkGray": "#1C1C1E",
          "black": "#000000"
        },
        "background": {
          "primary": "#000000",
          "secondary": "#1C1C1E",
          "card": "#2A2A2A",
          "translucent": "rgba(255, 255, 255, 0.1)"
        },
        "text": {
          "primary": "#FFFFFF",
          "secondary": "#8E8E93",
          "inverted": "#000000"
        }
      },
      "spacing": {
        "scale": {
          "xs": "4px",
          "sm": "8px",
          "md": "12px",
          "lg": "16px",
          "xl": "20px",
          "2xl": "24px",
          "3xl": "32px",
          "4xl": "48px"
        }
      },
      "shadows": {
        "none": "none",
        "sm": "0px 1px 3px rgba(0, 0, 0, 0.3)",
        "md": "0px 4px 8px rgba(0, 0, 0, 0.3)",
        "lg": "0px 8px 16px rgba(0, 0, 0, 0.4)"
      },
      "borderRadius": {
        "none": "0px",
        "sm": "4px",
        "md": "8px",
        "lg": "12px",
        "xl": "16px",
        "2xl": "20px",
        "full": "9999px"
      },
      "iconography": {
        "style": "filled",
        "strokeWidth": "2px",
        "size": "20px"
      },
      "interactionPatterns": {
        "hover": {
          "opacity": 0.8,
          "scale": 1.02
        },
        "active": {
          "opacity": 0.9,
          "scale": 0.98
        }
      }
    },
    "structure": {
      "layoutGrids": {
        "card": "flex column",
        "listItem": "flex row",
        "buttonGroup": "flex row with gap",
        "calendar": "7-column grid",
        "featureGrid": "2-column grid"
      },
      "hierarchy": {
        "card": {
          "header": "title/icon",
          "content": "main content",
          "footer": "actions/labels"
        },
        "listItem": {
          "icon": "left",
          "content": "center",
          "action": "right"
        },
        "featureCard": {
          "icon": "top",
          "title": "center",
          "description": "bottom"
        }
      },
      "alignmentRules": {
        "text": "left-aligned",
        "numbers": "right-aligned",
        "centered": "center-aligned",
        "vertical": "center-aligned"
      }
    },
    "components": {
      "Card": {
        "properties": {
          "backgroundColor": "var(--background-card)",
          "borderRadius": "var(--border-radius-lg)",
          "boxShadow": "var(--shadow-md)",
          "padding": "var(--spacing-lg)"
        },
        "states": {
          "default": {},
          "hover": {
            "boxShadow": "var(--shadow-lg)"
          }
        }
      },
      "FeatureCard": {
        "properties": {
          "backgroundColor": "var(--background-card)",
          "borderRadius": "var(--border-radius-lg)",
          "padding": "var(--spacing-lg)",
          "display": "flex",
          "flexDirection": "column",
          "alignItems": "center",
          "textAlign": "center"
        },
        "states": {
          "default": {},
          "hover": {
            "backgroundColor": "var(--background-secondary)"
          }
        }
      },
      "Button": {
        "variants": {
          "primary": {
            "backgroundColor": "var(--primary-blue)",
            "color": "var(--text-primary)",
            "borderRadius": "var(--border-radius-md)"
          },
          "secondary": {
            "backgroundColor": "var(--background-card)",
            "color": "var(--text-primary)",
            "borderRadius": "var(--border-radius-md)"
          },
          "circular": {
            "borderRadius": "var(--border-radius-full)",
            "width": "48px",
            "height": "48px"
          }
        },
        "states": {
          "default": {},
          "hover": {
            "opacity": 0.8
          },
          "active": {
            "opacity": 0.9
          }
        }
      },
      "Alert": {
        "variants": {
          "error": {
            "backgroundColor": "var(--primary-red)",
            "color": "var(--text-primary)",
            "borderRadius": "var(--border-radius-md)",
            "padding": "var(--spacing-md)",
            "display": "flex",
            "alignItems": "center",
            "gap": "var(--spacing-md)"
          }
        },
        "states": {
          "default": {}
        }
      },
      "Avatar": {
        "properties": {
          "borderRadius": "var(--border-radius-full)",
          "width": "40px",
          "height": "40px",
          "backgroundColor": "var(--background-card)"
        }
      }
    },
    "tokens": {
      "fontSizes": {
        "xs": "12px",
        "sm": "14px",
        "base": "16px",
        "lg": "18px",
        "xl": "20px",
        "2xl": "24px",
        "3xl": "32px",
        "4xl": "48px"
      },
      "fontWeights": {
        "regular": 400,
        "medium": 500,
        "semibold": 600,
        "bold": 700
      },
      "lineHeights": {
        "tight": 1.2,
        "default": 1.5,
        "relaxed": 1.6
      },
      "colorPalette": {
        "purple": "#8A63D2",
        "orange": "#FF9500",
        "green": "#34C759",
        "blue": "#007AFF",
        "yellow": "#FFCC00",
        "gray": "#8E8E93",
        "red": "#FF3B30",
        "pink": "#FF69B4",
        "white": "#FFFFFF",
        "lightGray": "#F2F2F7",
        "mediumGray": "#8E8E93",
        "darkGray": "#1C1C1E",
        "black": "#000000"
      },
      "spacingScale": {
        "4": "4px",
        "8": "8px",
        "12": "12px",
        "16": "16px",
        "20": "20px",
        "24": "24px",
        "32": "32px",
        "48": "48px"
      },
      "borderRadius": {
        "4": "4px",
        "8": "8px",
        "12": "12px",
        "16": "16px",
        "20": "20px",
        "full": "9999px"
      },
      "shadows": {
        "none": "none",
        "sm": "0px 1px 3px rgba(0, 0, 0, 0.3)",
        "md": "0px 4px 8px rgba(0, 0, 0, 0.3)",
        "lg": "0px 8px 16px rgba(0, 0, 0, 0.4)"
      }
    }
  }
}